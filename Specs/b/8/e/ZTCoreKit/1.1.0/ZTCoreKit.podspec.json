{
  "name": "ZTCoreKit",
  "version": "1.1.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "Official Zhortech ZTCoreKit SDK for iOS to access core features.",
  "homepage": "https://zhortech.com/",
  "social_media_url": "https://twitter.com/zhortech",
  "authors": {
    "Pavel Shpak": "p.shpak@zhortech.com"
  },
  "source": {
    "git": "https://github.com/zhortech/ztcorekit-ios-sdk.git",
    "tag": "1.1.0"
  },
  "platforms": {
    "ios": "13.0"
  },
  "requires_arc": true,
  "swift_versions": "5.0",
  "cocoapods_version": ">= 1.10.1",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "vendored_frameworks": "Sources/ZTCoreKit.xcframework",
  "ios": {
    "weak_frameworks": [
      "CoreBluetooth",
      "Security",
      "UIKit",
      "Foundation",
      "CFNetwork"
    ]
  },
  "exclude_files": [
    "readme-images/",
    "docs/"
  ],
  "swift_version": "5.0"
}
